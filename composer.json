{
    "name": "packages/import-errors",
    "description": "package for capture import errors",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "nitin",
            "email": "np@savitriya.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "packages\\import-errors\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ImportErrors\\Mailer\\ImportErrorsServiceProvider"
            ],
            "aliases": {
                "ImportErrors": "ImportErrors\\ImportDataError\\ImportErrorsFacade"
            }
        }
    }
}
